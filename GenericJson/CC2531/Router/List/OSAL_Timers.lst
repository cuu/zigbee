    593           * @brief
    594           *
    595           *   Search timer table to return the lowest timeout value. If the
    596           *   timer list is empty, then the returned timeout will be zero.
    597           *
    598           * @param   none
    599           *
    600           * @return  none
    601           *********************************************************************/
    602          uint16 osal_next_timeout( void )
    603          {
    604            uint16 nextTimeout;
    605            osalTimerRec_t *srchTimer;
    606          
    607            if ( timerHead != NULL )
    608            {
    609              // Head of the timer list
    610              srchTimer = timerHead;
    611              nextTimeout = OSAL_TIMERS_MAX_TIMEOUT;
    612          
    613              // Look for the next timeout timer
    614              while ( srchTimer != NULL )
    615              {
    616                if (srchTimer->timeout < nextTimeout)
    617                {
    618                  nextTimeout = srchTimer->timeout;
    619                }
    620                // Check next timer
    621                srchTimer = srchTimer->next;
    622              }
    623            }
    624            else
    625            {
    626              // No timers
    627              nextTimeout = 0;
    628            }
    629          
    630            return ( nextTimeout );
    631          }
